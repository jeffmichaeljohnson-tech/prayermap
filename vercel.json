{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    { "source": "/auth/callback", "destination": "/auth/callback/index.html" },
    { "source": "/((?!auth/callback).*)", "destination": "/" }
  ]
}
